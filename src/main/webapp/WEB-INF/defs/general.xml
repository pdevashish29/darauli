<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>
	<definition name="common" template="/WEB-INF/layout/classic.jsp">
		<put-attribute name="footer" value="/WEB-INF/layout/footer.jsp" />
		<put-attribute name="leftmenu" value="/WEB-INF/layout/leftmenu.jsp" />
		<put-attribute name="rightmenu" value="/WEB-INF/layout/rightmenu.jsp" />
		<put-attribute name="header" value="/WEB-INF/layout/header.jsp" />
	</definition>

	<definition name="welcome" extends="common">
		<put-attribute name="title" value="welcome" />
		<put-attribute name="body" value="/WEB-INF/jsp/welcome.jsp" />
		<put-attribute name="current" value="welcome" />
	</definition>
	
	<definition name="village" extends="common">
		<put-attribute name="title" value="welcome" />
		<put-attribute name="body" value="/WEB-INF/jsp/village.jsp" />
		<put-attribute name="current" value="welcome" />
	</definition>
	
	
	
	
	

</tiles-definitions>